{
  "name": "algoraisins",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "node --loader ts-node/esm",
    "test": "mocha --loader ts-node/esm",
    "test:all": "mocha --loader ts-node/esm './general/*/*/*.spec.ts' './raisins/*/*.spec.ts'",
    "test:general": "mocha --loader ts-node/esm './general/*/*.spec.ts'",
    "test:raisins": "mocha --loader ts-node/esm './raisins/*/*.spec.ts'",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prepare": "husky"
  },
  "devDependencies": {
    "@types/chai": "^5.0.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.5",
    "@types/sinon": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "chai": "^5.1.2",
    "eslint": "^9.17.0",
    "husky": "^9.1.7",
    "lodash": "^4.17.21",
    "mocha": "^11.0.1",
    "sinon": "^19.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "ts-node": {
    "esm": true,
    "experimentalSpecifierResolution": "node"
  }
}
